<?xml version="1.0" encoding="UTF-8" ?>
<OpenSimDocument Version="40000">
    <MuscleTendonPersonalizationPreprocessingTool>
        <!--Directory used for writing results.-->
        <results_directory>preprocessed</results_directory>
        <!--Directory for input files-->
        <input_directory/>
        <!--Name of the input model file (.osim) to use-->
        <input_model_file>CPRIT_Patient3_MuscleGroups.osim</input_model_file>
		<!-- Result .mot file of InverseKinematics -->
		<inverse_kinematics_file>ik.mot</inverse_kinematics_file>
		<!-- Result .mot file of InverseDynamics -->
		<inverse_dynamics_file>id.mot</inverse_dynamics_file>
		<!-- The .mot/.sto file containing time synchronized emg data-->
		<emg_file>processedEMGLeft.mot</emg_file>
		<!-- The directory containing MuscleAnalysis results-->
		<muscle_analysis_directory>MuscleAnalysis</muscle_analysis_directory>
		<!-- The space separated list of names of coordinates to be used in MTP-->
		<MTPCoordinateList>hip_adduction_l hip_flexion_l hip_rotation_l knee_angle_l ankle_angle_l subtalar_angle_l</MTPCoordinateList>
		<!-- The cutoff frequency for muscle tendon velocity GCV Spline -->
		<muscle_tendon_velocity_cutoff_frequency></muscle_tendon_velocity_cutoff_frequency>
		<!-- The list of start and end times of trials to be used in MTP -->
		<MTPTimePairList>
			<!-- Space separated group of start and end times for a given trial -->
			<MTPTimePair>0.593 1.667</MTPTimePair>
			<!-- Space separated group of start and end times for a given trial -->
			<MTPTimePair>1.667 2.767</MTPTimePair>
			<!-- Space separated group of start and end times for a given trial -->
			<MTPTimePair>2.767 3.827</MTPTimePair>
			<!-- Space separated group of start and end times for a given trial -->
			<MTPTimePair>3.827 4.907</MTPTimePair>
			<!-- Space separated group of start and end times for a given trial -->
			<MTPTimePair>4.907 5.993</MTPTimePair>
		</MTPTimePairList>
		<!-- Prefix (one string without space) to denote each trial (gait, squat, step_up), useful when multiple motions are used -->
		<MTPTrialPrefix>gait</MTPTrialPrefix>
		<!-- *optional* true if you want to process the emg data as part of preprocessing-->
		<preprocess_emg>true</preprocess_emg>
		<!-- *optional* filter degree for emg preprocessing -->
		<preprocess_emg_filter_degree>4</preprocess_emg_filter_degree>
		<!-- *optional* filter degree for emg preprocessing -->
		<preprocess_emg_high_pass_cutoff>10</preprocess_emg_high_pass_cutoff>
		<!-- *optional* filter degree for emg preprocessing -->
		<preprocess_emg_low_pass_cutoff>40</preprocess_emg_low_pass_cutoff>
    </MuscleTendonPersonalizationPreprocessingTool>
</OpenSimDocument>
